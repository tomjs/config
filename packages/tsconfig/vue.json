{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./vue-base.json",
  "display": "vue config",
  "compilerOptions": {
    /* Linting */
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "erasableSyntaxOnly": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedSideEffectImports": true
  }
}
